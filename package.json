{
  "name": "playwright-vsco-downloader",
  "version": "1.0.0",
  "description": "Playwright-based service to download images from VSCO using their API",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageManager": "pnpm@10.15.0",
  "bin": {
    "playwright-vsco-downloader": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "dev": "tsx src/cli.ts",
    "install-browsers": "playwright install",
    "download": "npm run build && node dist/cli.js",
    "download:headless": "npm run build && node dist/cli.js --headless",
    "download:debug": "npm run build && node dist/cli.js --debug",
    "download:dev": "tsx src/cli.ts",
    "download:dev:headless": "tsx src/cli.ts --headless",
    "download:dev:debug": "tsx src/cli.ts --debug",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test-browser": "tsx src/test-browser.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "playwright",
    "vsco",
    "image-downloader",
    "automation",
    "api"
  ],
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "playwright": "^1.55.0",
    "dotenv": "^17.2.1",
    "fs-extra": "^11.2.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "mime": "^4.0.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "typescript": "^5.7.2",
    "tsx": "^4.19.2",
    "@types/node": "^22.10.2",
    "@types/fs-extra": "^11.0.4"
  }
}
